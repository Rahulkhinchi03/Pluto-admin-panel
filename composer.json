{
    "name": "sertxudeveloper/lyra",
    "type": "library",
    "license": "MIT",
    "description": "An administration panel for Laravel for bootstrap your app and turn ideas into reality",
    "keywords": [
        "sertxudeveloper",
        "laravel",
        "lyra",
        "admin",
        "panel"
    ],
    "support": {
        "issues": "https://github.com/sertxudeveloper/Lyra/issues",
        "source": "https://github.com/sertxudeveloper/Lyra"
    },
    "homepage": "https://github.com/sertxudeveloper/Lyra",
    "authors": [
        {
            "name": "Sergio Peris",
            "email": "dev.sertxu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\Lyra\\": "src",
            "SertxuDeveloper\\Lyra\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SertxuDeveloper\\Lyra\\Tests\\": "tests",
            "SertxuDeveloper\\Lyra\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\Lyra\\LyraServiceProvider"
            ],
            "aliases": {
                "Lyra": "SertxuDeveloper\\Lyra\\Facades\\Lyra"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
